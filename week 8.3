a=int(input())
l=[]
l2=[]
for i in range(a):
    l.append(input())
for i in l:
    l1=i.split(" ")
    l2.append(l1)
avg=[]
z1=[]
z4=[]
for i in l2:
    ab=int(i[1])+int(i[2])+int(i[3])
    cd=ab/3
    avg.append(cd)
mavg=max(avg)
miavg=min(avg)
for i in range(len(avg)):
    if avg[i]==mavg:
        z1.append(l2[i][0])
    if avg[i]==miavg:
        z4.append(l2[i][0])
if len(z1)==1:
    print(z1[0])
else:
    z1.sort()
    print(z1[0],z1[1])
